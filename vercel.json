{
  "version": 2,
  "builds": [
    {
      "src": "server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/((?!.*\\.\\w+$).*)",
      "dest": "server.cjs"
    },
    {
      "src": "/(.+\\.[a-z]+)$",
      "dest": "/dist/$1"
    }
  ]
}
