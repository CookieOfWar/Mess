{
  "version": 2,
  "buildCommand": "npm run build",
  "builds": [
    {
      "src": "server.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/public",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "server.cjs"
    }
  ]
}
